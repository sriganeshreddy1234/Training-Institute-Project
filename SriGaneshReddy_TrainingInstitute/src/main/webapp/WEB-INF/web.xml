<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>SriGaneshReddy_TrainingInstitute</display-name>
	
	<welcome-file-list>
	<welcome-file>pages1\\index.jsp</welcome-file>
	<welcome-file>pages1\\error.jsp</welcome-file>
	<welcome-file>pages1\\AdminLogin.jsp</welcome-file>
	<welcome-file>pages1\\AdminHome.jsp</welcome-file>
	<welcome-file>pages1\\InstituteLogin.jsp</welcome-file>
	<welcome-file>pages1\\InstituteRegister.jsp</welcome-file>
	<welcome-file>pages1\\InstituteLoginSuccess</welcome-file>
	<welcome-file>pages1\\InstituteHome.jsp</welcome-file>
	<welcome-file>pages1\\StudentLogin.jsp</welcome-file>
	<welcome-file>pages1\\StudentRegister.jsp</welcome-file>
	<welcome-file>pages1\\StudentLoginSuccess.jsp</welcome-file>
	<welcome-file>pages1\\StudentHome.jsp</welcome-file>
	
	
	</welcome-file-list>
	
	 
	
	
	<servlet>
		<description></description>
		<display-name>MvcController</display-name>
		<servlet-name>MvcController</servlet-name>
		<servlet-class>com.nttdata.mvc.MvcController</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>MvcController</servlet-name> 
		<url-pattern>*.do</url-pattern>    
	</servlet-mapping>
	   
	  
	
	<resource-ref>
		<description>DB Connection</description>
		<res-ref-name>jdbc/mywebdesign</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	 
	 
	
	<listener>
		<listener-class>com.nttdata.training.web.handlers.MyListenerClass</listener-class>
	</listener>



</web-app>